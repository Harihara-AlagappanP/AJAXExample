<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>WorldDB</display-name>
  <welcome-file-list>
    <welcome-file>SearchBar.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>getDistricts</servlet-name>
  	<servlet-class>com.cities.servlet.DistrictsServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>getDistricts</servlet-name>
  	<url-pattern>/get-districts</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>getCities</servlet-name>
  	<servlet-class>com.cities.servlet.CitiesServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>getCities</servlet-name>
  	<url-pattern>/cities</url-pattern>
  </servlet-mapping>
  
</web-app>